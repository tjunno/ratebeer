<h2>List of ratings</h2>

<p> Number of ratings: <%= @ratings.count %> </p>

<ul>
 <% @ratings.each do |rating| %>
   <li> <%= rating %> </li> <%= link_to 'delete', rating, :method => :delete, data: { confirm: 'Are you sure?' }%> </li>
 <% end %>

  <br><br>
  <%= link_to 'New Rating', new_rating_path %>

</ul>